<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:androidprv="http://schemas.android.com/apk/prv/res/android" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:gravity="fill" android:id="@+id/actionbar_background">
        <shape android:tint="?attr/onePlusActionbarBackgroundColor" android:shape="rectangle">
            <solid android:color="@android:color/white"/>
        </shape>
    </item>
    <item android:gravity="bottom|left|right|center_vertical|center_horizontal|fill_horizontal|center" android:id="@+id/actionbar_divider">
        <shape android:tint="?attr/onePlusActionbarLineColor" android:shape="rectangle">
            <size android:height="@dimen/oneplus_contorl_divider_height_standard"/>
            <solid android:color="@android:color/white"/>
        </shape>
    </item>
</layer-list>

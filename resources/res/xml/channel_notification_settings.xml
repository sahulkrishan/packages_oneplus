        <com.android.settingslib.RestrictedSwitchPreference android:title="@string/app_notification_override_dnd_title" android:key="bypass_dnd" android:summary="@string/app_notification_override_dnd_summary" android:order="16" app:useAdditionalSummary="true"/>
        <Preference android:title="@string/app_settings_link" android:key="app_link" android:order="17" app:allowDividerAbove="true"/>
    </PreferenceCategory>
    <com.android.settings.notification.NotificationFooterPreference android:key="desc" android:order="100"/>
    <com.android.settings.notification.NotificationFooterPreference android:key="block_desc" android:order="110"/>
</PreferenceScreen>

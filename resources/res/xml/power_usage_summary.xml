<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:settings="http://schemas.android.com/apk/res/com.android.settings" xmlns:androidprv="http://schemas.android.com/apk/prv/res/android" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:title="@string/power_usage_summary_title" android:key="power_usage_summary_screen" app:keywords="@string/keywords_battery">
    <com.android.settings.applications.LayoutPreference android:layout="@layout/battery_header" android:selectable="false" android:key="battery_header"/>
    <PreferenceCategory android:layout="@layout/preference_category_no_title" android:key="battery_tip"/>
    <Preference android:title="@string/battery_saver" android:key="battery_saver_summary" android:fragment="com.android.settings.fuelgauge.batterysaver.BatterySaverSettings" app:allowDividerAbove="true" app:controller="com.android.settings.fuelgauge.BatterySaverController" app:platform_slice="true"/>
    <Preference android:persistent="false" android:title="@string/high_power_apps" android:key="op_power_optimize"/>
    <Preference android:title="@string/smart_battery_manager_title" android:key="smart_battery_manager" android:fragment="com.android.settings.fuelgauge.SmartBatterySettings" app:controller="com.android.settings.fuelgauge.batterytip.BatteryManagerPreferenceController"/>
    <com.android.settings.fuelgauge.PowerGaugePreference android:title="@string/battery_last_full_charge" android:selectable="false" android:key="last_full_charge" app:allowDividerAbove="true"/>
    <com.android.settings.fuelgauge.PowerGaugePreference android:title="@string/device_screen_usage" android:selectable="false" android:key="screen_usage"/>
</PreferenceScreen>

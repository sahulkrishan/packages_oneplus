<?xml version="1.0" encoding="utf-8"?>
<com.android.setupwizardlib.GlifLayout xmlns:settings="http://schemas.android.com/apk/res/com.android.settings" xmlns:androidprv="http://schemas.android.com/apk/prv/res/android" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:icon="@drawable/ic_opfinger_logo_bg" android:id="@+id/setup_wizard_layout" android:layout_width="match_parent" android:layout_height="match_parent" app:suwFooter="@layout/choose_lock_password_footer">
    <LinearLayout android:orientation="vertical" android:clipChildren="false" android:clipToPadding="false" android:layout_width="match_parent" android:layout_height="match_parent" android:paddingStart="@dimen/oneplus_contorl_layout_margin_left2" android:paddingEnd="@dimen/oneplus_contorl_layout_margin_right2">
        <FrameLayout android:layout_width="match_parent" android:layout_height="wrap_content">
            <TextView android:textColor="@color/oneplus_contorl_text_color_primary_dark" android:id="@+id/start_message" android:layout_width="match_parent" android:layout_height="wrap_content" android:minHeight="@dimen/oneplus_contorl_list_item_height_one_line1" android:text="@string/oneplus_fingerprint_enroll_summary" style="@style/OnePlusSuwGlifHeaderBodyStyle"/>
            <TextView android:textColor="@color/oneplus_contorl_text_color_primary_dark" android:id="@+id/repeat_message" android:visibility="invisible" android:layout_width="match_parent" android:layout_height="wrap_content" android:text="@string/security_settings_fingerprint_enroll_repeat_message" style="@style/OnePlusSuwGlifHeaderBodyStyle"/>
        </FrameLayout>
        <include android:layout_gravity="center" android:id="@+id/fingerprint_enroll_enrolling_anim" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="100dp" layout="@layout/fingerprint_enroll_enrolling_content"/>
        <TextView android:layout_gravity="center_horizontal" android:id="@+id/error_text" android:visibility="invisible" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="0dp" android:accessibilityLiveRegion="polite" style="@style/TextAppearance.FingerprintErrorText"/>
        <Button android:id="@+id/skip_button" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="@dimen/oneplus_contorl_layout_margin_top3" android:layout_marginBottom="@dimen/oneplus_contorl_layout_margin_bottom1" android:text="@string/skip_label" style="@style/SetupWizardButton.Negative"/>
    </LinearLayout>
</com.android.setupwizardlib.GlifLayout>

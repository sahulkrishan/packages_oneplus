<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:androidprv="http://schemas.android.com/apk/prv/res/android" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:state_focused="true" android:state_checked="true">
        <shape android:shape="rectangle">
            <solid android:color="?attr/opDropdownListItemSelectedColor"/>
        </shape>
    </item>
    <item android:state_focused="false" android:state_checked="true">
        <shape android:shape="rectangle">
            <solid android:color="?attr/opDropdownListItemSelectedColor"/>
        </shape>
    </item>
</selector>
